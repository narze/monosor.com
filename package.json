{
  "name": "gatsby-starter-tailwind",
  "description": "Gatsby starter styled with Tailwind",
  "version": "4.0.0",
  "author": "Taylor Bryant <taylor@bryant.io>",
  "dependencies": {
    "gatsby": "^2.24.67",
    "gatsby-plugin-manifest": "^2.4.33",
    "gatsby-plugin-offline": "^3.2.30",
    "gatsby-plugin-postcss": "^3.0.0",
    "gatsby-plugin-react-helmet": "^3.3.12",
    "gatsby-plugin-sharp": "^2.6.38",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0"
  },
  "keywords": [
    "gatsby",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "license": "MIT",
  "scripts": {
    "analyze:lint": "eslint --ext .jsx --ext .js .",
    "analyze:prettier": "prettier --list-different \"**/*.{css,js,jsx,json,md}\"",
    "analyze": "yarn run analyze:lint && yarn run analyze:prettier",
    "fix:lint": "eslint --ext .jsx --ext .js . --fix",
    "fix:prettier": "prettier --write \"**/*.{css,js,jsx,json,md}\"",
    "fix": "yarn run fix:lint && yarn run fix:prettier",
    "build": "gatsby build",
    "develop": "gatsby develop",
    "dev": "yarn run develop",
    "start": "yarn run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "cy:open": "cypress open",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "deploy": "gatsby build && gh-pages -d public -b master"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^1.6.18",
    "@mdx-js/react": "^1.6.18",
    "@tailwindcss/custom-forms": "0.2.1",
    "@testing-library/cypress": "^7.0.1",
    "autoprefixer": "^10.0.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "^26.5.0",
    "babel-preset-gatsby": "^0.5.12",
    "cssnano": "4.1.10",
    "cypress": "^5.3.0",
    "eslint": "^7.10.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-react": "^7.21.3",
    "gatsby-plugin-eslint": "2.0.8",
    "gatsby-plugin-mdx": "^1.2.43",
    "gatsby-source-filesystem": "^2.3.32",
    "gh-pages": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.0",
    "mkdirp": "^1.0.4",
    "prettier": "2.1.2",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.5",
    "tailwindcss": "^1.8.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind"
  },
  "bugs": {
    "url": "https://github.com/taylorbryant/gatsby-starter-tailwind/issues"
  }
}
